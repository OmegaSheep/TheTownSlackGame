<script>
    $(document).ready(function () {
        $.ajax({
           url: "/gamedata"
        })
        .done(function( data ) {
           console.log( "Sample of data:", data );
           dataString = data.message;
           if (dataString != "") {
             dataList = dataString.split("##");
             gameHeader = "";
             for (var i = 0; i < dataList.length; ++i) {
                if (i == 0) {
                  gameHeader += dataList[i];
                } else if (i == 1) {
                  gameHeader += " - Day " + dataList[i];
                  $("#gameHead").text(gameHeader);
                } else {
                  $("#gameData").append('<li class="center-justified gameList">'+dataList[i]+'</li>');
                }
             }
           }
        });
    });
</script>

